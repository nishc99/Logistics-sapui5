<core:FragmentDefinition
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.suite.ui.microchart"
	xmlns:m="sap.m"
	xmlns:layout="sap.ui.layout"
	>
	
	<layout:Grid defaultSpan="XL6 L6 M6 S12">

		
		<m:Text>
			
			<m:layoutData>
				<layout:GridData span="XL12 L12 M12 S12"/>
			</m:layoutData>
		</m:Text>
		<m:FlexBox width="30rem" height="20rem" alignItems="Center" class="sapUiSmallMargin">
			<m:items>
				<InteractiveBarChart labelWidth="25%" selectionChanged="selectionChanged" press="press">
					<bars>
						<InteractiveBarChartBar label="2020" value="50000" displayedValue="50%"/>
						<InteractiveBarChartBar label="2021" value="10000" displayedValue="10%"/>
						<InteractiveBarChartBar label="2022" value="20000" displayedValue="20%"/>
					</bars>
				</InteractiveBarChart>
			</m:items>
		</m:FlexBox>
		<m:FlexBox width="30rem" height="20rem" alignItems="Center" class="sapUiSmallMargin">
			<m:items>
				<InteractiveBarChart labelWidth="25%" selectionChanged="selectionChanged" press="press">
					<bars>
						<InteractiveBarChartBar label="2022-10" value="62,000" displayedValue="10%"/>
						<InteractiveBarChartBar label="2022-11" value="20" displayedValue="15%"/>
						<InteractiveBarChartBar label="2022-12" value="70" displayedValue="5%"/>
					</bars>
				</InteractiveBarChart>
			</m:items>
		</m:FlexBox>
	</layout:Grid>
</core:FragmentDefinition>